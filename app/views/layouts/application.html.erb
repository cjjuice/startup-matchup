<!DOCTYPE html>
<html>
<head>
  <title>StartupMatchup</title>
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="top_bar">
	</div>	
	<div id="header">
		<div id="nav_wrapper">
			<div id="navigation">
				<div id="logo">
					<a>
						<img src="/assets/logo.png"/>
					</a>	
				</div>		
				<div class="nav">
					<ul>
						<li id="nav_about">
							<a id="about_link">About</a>
						</li> 	
						<li id="nav_signup">
							<a id="signup_link">Signup</a>
						</li>
						<li id="nav_cal">
							<a id="cal_link">Dates</a>
						</li>
						<li id="nav_contact">
							<a id="contact_link">Contact</a>
						</li>
					</ul>	
				</div>
			</div>			
		</div>	
	</div>
	<div id="container_wrapper">			
<%= yield %>
	</div>

</body>
</html>
